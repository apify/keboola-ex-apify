  },
  "homepage": "https://github.com/apifytech/keboola-ex-apify#readme",
  "devDependencies": {
    "@apify/eslint-config": "^0.3.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.40.0",
    "mocha": "^10.2.0",
    "sinon": "^15.0.4"
  },
  "dependencies": {
    "@apify/consts": "^2.14.0",
    "@apify/utilities": "^2.4.1",
    "apify-client": "2.12.6-beta.5",
